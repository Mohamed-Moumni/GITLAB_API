<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="acces_category_backup_tracker" model="ir.module.category">
            <field name="description">Backup Tracker</field>
            <field name="name">Backup Tracker</field>
            <field name="sequence">9</field>
        </record>
        <record id="group_user" model="res.groups">
            <field name="name">User</field>
            <field name="category_id"
                   ref="kzm_backup_tracker.acces_category_backup_tracker"/>
        </record>
        <record id="group_manager" model="res.groups">
            <field name="name">Manager</field>
            <field name="implied_ids"
                   eval="[(6, 0, [ref('kzm_backup_tracker.group_user'),])]"/>
            <field name="category_id"
                   ref="kzm_backup_tracker.acces_category_backup_tracker"/>
            <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>

        </record>
        <!--<record id="group_mail_recap_backup" model="res.groups">
            <field name="name">Mail Recap Backup</field>
            <field name="implied_ids" eval="[(4, ref('kzm_backup_tracker.group_manager'))]"/>
            <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>

        </record>-->
    </data>
</odoo>

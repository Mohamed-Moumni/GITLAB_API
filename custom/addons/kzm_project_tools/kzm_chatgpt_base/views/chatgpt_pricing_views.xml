<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="chatgpt_pricing_tree" model="ir.ui.view">
        <field name="name">chatgpt.pricing.tree</field>
        <field name="model">chatgpt.pricing</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="model"/>
                <field name="price"/>
                <field name="tokens"/>
            </tree>
        </field>
    </record>

    <record id="chatgpt_pricing_action" model="ir.actions.act_window">
        <field name="name">Pricing</field>
        <field name="res_model">chatgpt.pricing</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>

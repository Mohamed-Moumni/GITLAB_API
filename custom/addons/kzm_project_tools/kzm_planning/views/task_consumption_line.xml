<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record model="ir.ui.view" id="taks_consumption_line_list_view">
            <field name="name">Task Consumption Line</field>
            <field name="model">task.consumption.line</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="ticket_id" readonly="1"/>
                    <field name="created_on" readonly="1"/>
                    <field name="subject" readonly="1"/>
                    <field name="type_id"/>
                    <field name="estimated_charge"/>
                    <field name="state"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>